<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/_bg__header"
        android:layout_width="386dp"
        android:layout_height="46dp"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="16dp"
        tools:ignore="MissingConstraints" >

        <TextView android:id="@+id/profile"
            android:textStyle="bold"
            android:layout_gravity="center_horizontal|center"
            android:gravity="center"
            android:textSize="16sp"
            android:layout_marginTop="12dp"
            android:layout_width="64dp"
            android:layout_height="28.6dp"
            android:text="Profile" />

        <View
            android:id="@+id/_bg__frame"
            android:background="@drawable/profileapp"
            android:layout_gravity="end"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_width="30dp"
            android:layout_height="32.76dp" />

        <View
            android:id="@+id/_bg__frame2"
            android:background="@drawable/arrow_left"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_width="30dp"
            android:layout_height="32.76dp" />
    </androidx.appcompat.widget.Toolbar>



    <androidx.cardview.widget.CardView
        android:id="@+id/foodCard1"
        android:layout_width="382dp"
        android:layout_height="200dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:background="@color/gray"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/_bg__header">



        <TextView
            android:id="@+id/foodName1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            android:text="Weight"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:fontFamily="@font/poppins_semi_bold" />

        <TextView
            android:id="@+id/foodName2"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="50dp"
            android:layout_width="55dp"
            android:layout_height="25dp"
            android:text="54 kg"
            android:textColor="@color/black"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/line_chart"
            android:background="@drawable/line_chart"
            android:layout_marginStart="16dp"
            android:layout_marginTop="80dp"
            android:layout_width="358dp"
            android:layout_height="89dp" />
        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginStart="270dp"
            android:text="-7.5 kg"
            android:background="@drawable/rounded_button_2"/>

    </androidx.cardview.widget.CardView>

    <TextView android:id="@+id/text1"
        android:textSize="16sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Progress"
        android:fontFamily="@font/poppins_semi_bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/foodCard1" />

    <androidx.cardview.widget.CardView
        android:id="@+id/foodCard2"
        android:layout_width="382dp"
        android:layout_height="80dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:background="#34CE6C"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text1">


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="418dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:scaleType="centerCrop"
            android:src="@drawable/rectangle" />

        <TextView
            android:id="@+id/titleTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="45dp"
            android:text="Calories"
            android:textSize="18sp"
            android:textColor="@color/white"
            android:fontFamily="@font/poppins_medium" />

        <TextView
            android:id="@+id/descriptionTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:layout_marginStart="100dp"
            android:fontFamily="@font/poppins_semi_bold"
            android:text="20kcal"
            android:elevation="14sp"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:textSize="30sp" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/foodCard3"
        android:layout_width="382dp"
        android:layout_height="80dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:background="#34CE6C"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/foodCard2">


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="418dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:scaleType="centerCrop"
            android:src="@drawable/rectangle2" />

        <TextView
            android:id="@+id/titleTextView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="45dp"
            android:text="Carbs"
            android:textSize="18sp"
            android:textColor="@color/white"
            android:fontFamily="@font/poppins_medium" />

        <TextView
            android:id="@+id/descriptionTextView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:layout_marginStart="120dp"
            android:fontFamily="@font/poppins_semi_bold"
            android:text="20g️"
            android:elevation="14sp"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:textSize="30sp" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/foodCard5"
        android:layout_width="382dp"
        android:layout_height="80dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:background="#34CE6C"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/foodCard3">


        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="418dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:scaleType="centerCrop"
            android:src="@drawable/rectangle3" />

        <TextView
            android:id="@+id/titleTextView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="45dp"
            android:text="Fat"
            android:textSize="18sp"
            android:textColor="@color/white"
            android:fontFamily="@font/poppins_medium" />

        <TextView
            android:id="@+id/descriptionTextView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:layout_marginStart="120dp"
            android:fontFamily="@font/poppins_semi_bold"
            android:text="20g️"
            android:elevation="14sp"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:textSize="30sp" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/foodcard"
        android:layout_width="382dp"
        android:layout_height="80dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:background="#34CE6C"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/foodCard5">


        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="418dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:scaleType="centerCrop"
            android:src="@drawable/rectangle4" />

        <TextView
            android:id="@+id/titleTextView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="45dp"
            android:text="Protein"
            android:textSize="18sp"
            android:textColor="@color/white"
            android:fontFamily="@font/poppins_medium" />

        <TextView
            android:id="@+id/descriptionTextView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:layout_marginStart="120dp"
            android:fontFamily="@font/poppins_semi_bold"
            android:text="20g️"
            android:elevation="14sp"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:textSize="30sp" />

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>